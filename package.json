{
  "name": "redux-transaction",
  "version": "0.0.2",
  "description": "Optimistically apply actions, then rollback when they fail",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src && eslint test",
    "build": "npm run lint && rm -rf lib && babel src -d lib",
    "build_tests": "rm -rf temp && babel test -d temp",
    "clean": "rimraf ./lib; rimraf ./temp;",
    "test": "npm run build && npm run build_tests && mocha temp",
    "prepublish": "npm test"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayphelps/redux-transaction.git"
  },
  "keywords": [
    "optimisic",
    "async",
    "actions",
    "transactions",
    "rollback",
    "revert",
    "commit",
    "begin",
    "fulfill",
    "reject",
    "cancel"
  ],
  "contributors": [
    {
      "name": "Jay Phelps",
      "email": "hello@jayphelps.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/jayphelps/redux-transaction/issues"
  },
  "homepage": "https://github.com/jayphelps/redux-transaction#README.md",
  "peerDependencies": {
    "redux": "3.*"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "install": "0.8.1",
    "mocha": "^2.4.5",
    "npm": "3.9.3",
    "redux": "^3.5.1",
    "rimraf": "^2.5.2"
  }
}
